{
  "name": "deadzone-dev-launcher",
  "version": "1.0.0",
  "description": "A Custom Launcher focused on local development for the Dead Zone Revive Project.",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . \"DeadZone DEV Launcher\" --out=build --platform=win32 --arch=x64 --icon=./assets/logo.ico --extraResource=./assets/logo.png --overwrite",
    "pack": "build --dir"
  },
