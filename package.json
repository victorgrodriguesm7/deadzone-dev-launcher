  "devDependencies": {
    "electron": "^11.5.0",
    "electron-rebuild": "^3.2.9"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "electron-packager": "^17.1.2",
    "express": "^4.17.1"
  }
